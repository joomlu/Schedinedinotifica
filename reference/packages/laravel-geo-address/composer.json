{
  "name": "tanggo/laravel-geo-address",
  "description": "Componenti Blade e asset JS per relazione geografica (GeoSelect) e campi indirizzo riutilizzabili in Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Tanggo\\GeoAddress\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tanggo\\GeoAddress\\Providers\\GeoAddressServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "minimum-stability": "stable"
}
